{
    "name": "hesamrad/laravel-flashlight",
    "description": "A Laravel Package to Log Requests",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/hesamzakerirad/laravel-flashlight",
    "keywords": [
        "laravel",
        "php",
        "log",
        "http"
    ],
    "authors": [
        {
            "name": "Hesam Rad",
            "email": "hesamzakerirad@gmail.com",
            "homepage": "https://webly.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/config": "^8.0 || ^9.0",
        "illuminate/database": "^8.53 || ^9.0",
        "illuminate/support": "^8.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "HesamRad\\Flashlight\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HesamRad\\Flashlight\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HesamRad\\Flashlight\\FlashlightServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
