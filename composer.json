{
    "name": "hesamrad/laravel-flashlight",
    "description": "A Laravel Package to Log Requests",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Hesamrad\\Flashlight\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HesamRad\\Flashlight\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Hesam Rad",
            "email": "hesamzakerirad@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "HesamRad\\Flashlight\\FlashlightServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
